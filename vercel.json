{
  "version": 2,
  "buildCommand": "npm ci && npm run -w apps/web build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm ci",
  "framework": null,
  "env": {
    "VITE_BUILDER_CODE": "builderscan"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

